    \field{title}{A program for annotating and predicting the effects of single
  nucleotide polymorphisms, SnpEff: SNPs in the genome of Drosophila
  melanogaster strain w1118; iso-2; iso-3}
    \field{volume}{6}
    \field{journaltitle}{Fly}
    \field{year}{2012}
  \endentry

